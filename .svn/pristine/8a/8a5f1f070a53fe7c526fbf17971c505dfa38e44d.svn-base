
.sn-table {
    border: 1px solid #e0e0e0;
    width: 100%;
    border-collapse: collapse
}

.sn-table th {
    font-size: 14px;
    color: #687B93;
    font-weight: 700
}

.sn-table td,
.sn-table th {
    height: 38px;
    text-align: left;
    padding-left: 40px;
}

.sn-table td {
    font-size: 12px;
    padding: 6px 0;
    padding-left: 40px;
}
.sn-table td:last-of-type{
    font-size: 12px;
    color: #333;
    padding: 6px 0;
    padding-left: 40px;
    text-align: center
}
.sn-table td a {
    color: #11B983;
    margin: 0 10px;
    cursor: pointer
}

.sn-table td:first-of-type,
th:first-of-type {
    padding-left: 20px;
    text-align: center
}
th:last-of-type {
    width: 150px;
    text-align: center
}


.sn-table tr:nth-child(odd) {
    background-color: #F6F7F9;
}

.sn-table tr:first-child {
    background-color: #F6F7F9;
}

.sn-table-btn-group {
    width: 35%
}

.sn-table-pager {
    width: 65%
}

.sn-table-pager ul {
    margin: 0
}

.table-text {
    margin-top: 10px;
    font-size: 12px;
    color: #333
}

.table-text a {
    margin-left: 20px;
    color: #3a8cbb;
    cursor: pointer
}
td {
    border: 0;
}






.main-div{
    min-width: 825px;
    min-height: 468px;
}
.search-div{
    background-color: #11B983;
    color: #fff;
    min-width: 60px;
    height: 26px;
    display: inline-block;
    text-align: center;
    cursor: pointer;
    vertical-align: middle;
    font-size: 12px;
    line-height: 26px;

}
.reset-div{
    display: inline-block;
    text-align: center;
    cursor: pointer;
    vertical-align: middle;
    color: #11B983;
    font-size: 12px;
}
.marginL {
    margin-left: 6px;
}
.relative-div{
    position: relative;
    min-width: 150px;
    border: 1px solid #E0E3EA;
    margin: 10px;
}
.absolute-div{
    left: 40px;
    position: absolute;
    top: -10px;
    background-color: white;
    border-left: 10px solid white;
    border-right: 10px solid white;
    padding-left: 2px;
    padding-right: 2px;
    color: #5A5C80;
    font-size: 14;
}
.content-div{
    padding: 6px 6px 0px 6px;
    min-height: 76px;

}  
.button-div{
    float: left;
    margin-left: 35px;
    margin-bottom: 15px;
    margin-top: 10px;
}
.button-div a{
    color: #11B983;
}
.promanage{
    font-size: 18px;
   
}
.tr-style {
    font-size: 14px;
    color: #687B93;
    line-height: 18px;
    height: 18px;
    font-weight: normal;
    font-family: 'MicrosoftYaHeiUILight';
}

.sn-table a{
    color: #11B983;
    margin: 0 10px;
    cursor: pointer;
}
.lookMore{
    float: right;
    margin: 8px 6px;
    color: #11B983;
    font-size: 12px;
    cursor: pointer;
}
.system-div{
    display: inline-block;
    margin-left: 15px;
    padding-top: 7px;
    font-size: 14px;
}
.relative-div-top{
    position: relative;
    min-width: 150px;
    border: 1px solid #E0E3EA;
    margin: 10px;
    padding-left: 25px;
    overflow: hidden;
}


/***
reset css for navbar start
***/

.content-wrapper,
.right-side,
.main-footer {
	margin-left: 215px;
}

.nav-content {
	background: #FFFFFF;
	/*box-shadow: 0 2px 4px 0 #D8DDE9;*/
}

sn-breadcrumb>ul>li>a {
	font-family: MicrosoftYaHeiUILight;
	font-size: 12px;
	color: #686868;
}

sn-breadcrumb>ul>li>a:hover {
	font-family: MicrosoftYaHeiUILight;
	font-size: 12px;
	color: #11B983;
}

sn-breadcrumb>ul>li>span {
	font-family: MicrosoftYaHeiUILight;
	font-size: 12px;
	color: #11B983;
}

.nav-title-decoration-color {
    display: inline-block;
    height: 28px;
    width: 6px;
    background-color: #11B983;
    float: left;
    margin-right: 18px;
    margin-left: -15px;
}

.nav-title {
	padding: 13px 0px 0px 15px;
	border-bottom: 5px solid #dee3ec;
}

.nav-title p {
	margin: 6px 0 0 24px;
}

.nav-title-name {
	font-family: MicrosoftYaHeiUILight;
	font-size: 18px;
	color: #000000;
    overflow: hidden;
    margin-bottom: 27px;
}


.select2height{
    overflow: hidden;
    height: 26px;
    border-bottom: 1px solid #ddd;
}
.select2-container--default .select2-selection--single, .select2-selection .select2-selection--single {
    padding: 4px 12px;
}
input {
    height: 26px;
    padding-left: 10px;
    padding-right: 0px;
    border: 1px solid #ddd;
    color: #333;
    width: 170px;
}
.mainContent{
    width: 99%;
    margin-left: 10px;
}
input.form-control {
    height: 28px;
}
.text_overflow {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    width: 100px;
}
.horButton{
    float: right;
    font-size: 12px;
    margin-top: 3px;
    margin-right: 20px;
}
input.sheet-input, select.sheet-select{
    min-width:160px;
    height:26px;
    padding-top:0;
    padding-bottom:0;
    color:#c9c9c9;
}
input.sheet-input::-webkit-input-placeholder{
    color:#c9c9c9;
}

.ux-radio-inline{
    padding-top: 7px;
}

.ux-radio-inner{
    font-size: 14px;
}
.mainbox{
    overflow: hidden;
    background-color: #E8EBF2;
    margin: 0;
    padding: 0;
}
.boxStyle{
    float: left;
    width: 85px;
    height: 27px;
    border-radius: 23px;
    background-color: #fff;
    text-align: center;
    line-height: 27px;
    margin-top: 15px;
    margin-bottom: 15px;
    margin-left: 26px;
    cursor: pointer;
}
.boxStyleUncheck{
    float: left;
    width: 85px;
    height: 27px;
    border-radius: 23px;
    background-color: #11B983;
    color: #fff;
    text-align: center;
    line-height: 27px;
    margin-top: 15px;
    margin-bottom: 15px;
    margin-left: 26px;
    cursor: pointer;
}