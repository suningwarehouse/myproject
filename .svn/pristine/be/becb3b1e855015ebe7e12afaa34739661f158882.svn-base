.cpanel div.icon span{-moz-transition-duration: 0.8s;background-color: #67b467;background-position: -30px 50%;border: 1px solid #67b467;border-radius: 60px;color: #fff;display: block;float: left;height: 35px;width: 35px;text-align:center;text-indent:0;padding-top:9px;margin-bottom:15px;font-size:12px;}
.cpanel div.icon span:hover{border:1px solid #67b467;}

.cpanel div.icon-blue span{-moz-transition-duration: 0.8s;background-color: #6690c4;background-position: -30px 50%;border: 1px solid #6690c4;border-radius: 60px;color: #fff;display: block;float: left;height: 35px;width: 35px;text-align:center;text-indent:0;padding-top:11px;margin-bottom:15px;font-size:12px;}
.cpanel div.icon-blue span:hover{border:1px solid #6690c4;}

.cpanel div.icon-pic {
	width:22px;
	height:22px;
	background:url("../css/images/publish/step_node.png") ;	/*#0081CC;*/
	display:block;
	margin:30px 6px;
 }

.btn{}
.btn.btn-danger{margin:20px;}
.panel-default-left{margin:0 0 0 20px;padding-bottom:25px;}

.list-group-item{border-radius:0px;}

.package-title{width:100%;height:28px;border:1px solid #67b467;background-color:#67b467;color:#fff;padding:2px 0 0 5px;margin-top:5px;}
.package-publish-title{width:100%;height:28px;background-color:#f0f1f6;margin-top:5px;}
.form-group-width{width:80px;color:#7c91a0;font-weight:bold;}

table{width:100%;text-align:center;}
table, td, th
  {
  	border:1px solid #c7d1d8;
	font-size:12px;
  }

th
{
  background-color:#e3e9ed;
  height:30px;
  text-align:center;
}
td
{
  height:35px;
  word-wrap:break-word;word-break:break-all;
}
.tb-noborder{border:0px;text-align:left;}
.tb-noborder td{border:0px;}
.tb-noborder td{border:0px;}

.tb-noborder td:first-child
{
	font-weight:bold
}
.deploy-success{
	width:518px;
	height:178px;
	background:url("../css/images/publish/deploy_success.png") ;	/*#0081CC;*/
	display:block;
	margin:200px 6px;
}
.deploy-success .btn{
	margin:130px 0 0 220px;
}
.deploy-log-detail{padding:10px 20px;text-align:left;}
