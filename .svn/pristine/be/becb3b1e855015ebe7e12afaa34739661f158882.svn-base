/*
* 发布模块样式
*/
.pl-10{padding-left:10px;}
.container-bg{background-color:#f0f1f6;margin:10px 0 0 -10px;padding-bottom:15px;}
.publish-title{background-color:#f0f1f6;margin:10px;display:inline;}
.publish-title span{background-color:#337ab7;width:3px;height:15px;display:block;float:left;margin:3px 0 0 -5px;}
.container-body-bg{background-color:#fff;padding-bottom:15px;}
