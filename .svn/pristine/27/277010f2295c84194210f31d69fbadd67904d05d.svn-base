}

.relative-div{
    float: left;
    position: relative;
    min-width: 150px;
    border: 1px solid #E0E3EA;
    margin-bottom: 10px;
    width: 79%;
}
.absolute-div{
    left: 40px;
    position: absolute;
    top: -10px;
    background-color: white;
    border-left: 10px solid white;
    border-right: 10px solid white;
    padding-left: 2px;
    padding-right: 2px;
    color: #5A5C80;
    font-size: 14;
}
.content-div{
    padding: 24px;
    min-height: 76px;

}  
.contentRight{
    margin-left: 30px;
}
.eventNotice{
    position: relative;
    border-left: 1px solid #ccc;
    min-height: 50px;
    margin-left: 20px;
}
.systemNotice{
    margin-bottom: 15px;
}
    
.circle-div{
    left: -5px;
    position: absolute;
    top: -5px;
    background-color: #666666;
    border-radius: 50%;
    width: 9px;
    height: 9px;
}
.lookMore{
    margin-left: 10px;
}
.sn-table{
    border: 1px solid #e0e0e0;
    width: 100%;
    border-collapse: collapse;
}
.sn-table tr:first-child {
    background-color: #eaedf1;
}

.sn-table td, .sn-table th {
    height: 38px;
    padding-left: 20px;
    text-align: left;
    font-size: 12px;
}
.sn-table th {
    font-size: 14px;
    color: #000;
    font-weight: 700;
}
.table-body {
    padding: 10px 0;
}
ul.a {
    list-style-type:none;
    display: inline-block;
    margin-top: 20px;
    padding-right: 30px;
    border-right: 2px solid #ccc;
}
ul.a li{
    margin-top: 10px;
    font-size: 16px;
    text-align: right;
    }
.rightContent{
    display: inline-block;
    margin-left: 20px;
    vertical-align: top;
    margin-top: 30px;
}
.leftList{
    position: relative;
    padding: 0px 0px 0px 0px;
}
.verticalMenu{
    position: static;
    display: inline-block;
    margin-left: 15px;
    margin-top: 30px;
    width: 18%;
}
.bottom-div{
    margin-top: 10px;
