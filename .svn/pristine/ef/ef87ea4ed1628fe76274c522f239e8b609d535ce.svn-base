<ul style="padding-left:15px;">
    <li ng-repeat="data in datalist" ng-switch="$last" ng-class="{'active': $last}">
        <a ng-switch-when="false" ng-click="reset($index)" ng-href="{{data.href}}">
            <span ng-bind-html="data.pre"></span> {{data.title}}
        </a>
        <span ng-switch-when="true">
        <span ng-bind-html="data.pre"></span> {{data.title}}
        </span>
        <span class="divider" ng-hide="$last"></span>
    </li>
</ul>
