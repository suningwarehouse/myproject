.table-border-mine {
    border: 1px solid lightgray;
    text-align: center;
    margin-top: 20px;
    width: 100%;
}

.table-border-mine th,
td {
    border: 1px solid lightgray;
    text-align: center;
    padding: 7px !important;
}

.info {
    background-color: #D9EDF7;
}

.margin-right-20 {
    margin-right: 20px
}

.margin-right-10 {
    margin-right: 10px
}

.width-180 {
    width: 180px
}

.table-body {
    padding: 10px 0
}

.sn-table {
    border: 1px solid #e0e0e0;
    width: 100%;
    border-collapse: collapse
}

.sn-table th {
    font-size: 14px;
    color: #000;
    font-weight: 700
}

.sn-table td,
.sn-table th {
    border: 1px solid #e0e0e0;
    height: 38px;
    text-align: center
}

.sn-table td {
    /*font-size: 12px;*/
    color: #333;
    padding: 6px 0
}

.sn-table td a {
    color: #3a8cbb;
    margin: 0 10px;
    cursor: pointer
}

.sn-table th:nth-child(3) {
    width: 200px
}

.sn-table tr:nth-child(odd) {
    background-color: #f0f2f5
}

.sn-table tr:first-child {
    background-color: #eaedf1
}

.sn-table-btn-group {
    width: 35%
}

.sn-table-pager {
    width: 65%
}

.sn-table-pager ul {
    margin: 0
}

.table-text {
    margin-top: 10px;
    font-size: 12px;
    color: #333
}

.table-text a {
    margin-left: 20px;
    color: #3a8cbb;
    cursor: pointer
}


.error {
    color: #f60;
}

.success {
    color: #78be25;
}


.clear {
    clear: both
}

.btn{
    padding: 6px 12px!important;
}

.text_overflow {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    width: 100px;
}
.text_change {
    width:700px;
    word-break:break-all;
}
.label-style{
    width: 9%;
    text-align: right;
}

.pop-main{
    display:inline-block;
    min-width:100px;
    height:278px;
    margin:0 auto;
    padding-top:10px;
    white-space: nowrap;
}
.pop-main>div{
        display:inline-block;
    min-width: 234px;
    height: 256px;
}
.pop-body-left{
    float: left;
    height: 100%;
    width: 15%;
    border-right: 1px solid green;
    padding-right: 35px;
    padding-left: 10px;
    vertical-align: middle;
    padding-top: 75px;
    white-space:normal; 
}

.pop-body-right span{
    color:red;
    padding-left:10px;
}
.pop-body-right{
        float: left;
    height: 94%;
    padding-top: 20px;
}
.pop-body-right ul{
    position: relative;
    left:-20px;
    height:100%;
    overflow-y: scroll;
}
.pop-main p{
        display: block;
    margin: 0 auto;
    width: 90%;
    border-top: 1px solid red;
    
}
.pop-p{
        position: relative;
    top: -3px;
    right: -20px;
}
.pop-p1{
        position: relative;
    top: -2px;
    right: -20px;
}
.results-detial {
   
    bottom: -17%;
    left: 30px;
}
.pop-body-right ul li{
    overflow:hidden;
}
.pop-body-right ul li a,.pop-body-right ul li span{
    float:left;
}

.modal h4{
    text-align:left;
}
.modal-header {
    margin-bottom: 31px !important;
}

.btn.btn-primary.reverse, .btn.btn-primary.reverse:hover {
    position: relative;
    top: 12px;
}









