.table-border-mine {
    border: 1px solid #E1E4EA;
    text-align:left;
    margin-top: 20px;
    width: 100%;
}

.table-border-mine th,
td {
    border: 1px solid lightgray;
    text-align: left;
    padding: 7px !important;
}

.info {
    background-color: #D9EDF7;
}

.margin-right-20 {
    margin-right: 20px
}

.margin-right-10 {
    margin-right: 10px
}

.width-180 {
    width: 180px
}

.table-body {
    padding: 10px 0
}

.sn-table {
    border: 1px solid #e0e0e0;
    width: 100%;
    border-collapse: collapse
}

.sn-table th {
    font-size: 14px;
    color: #000;
    font-weight: 700
}

.sn-table td,
.sn-table th {
    border: none;
    height: 38px;
    text-align: left;
    padding-left:5% !important;
}
.sn-table th{
    background:#F6F7F9;
    font-weight:normal;
    color:#687B93;
}
.sn-table td {
    /*font-size: 12px;*/
    color: #303C4A ;
    padding: 6px 0
}

.sn-table td a {
       color: #11B983;
    margin: 0 10px;
    cursor: pointer;
    padding: 0;
    margin: 0;
    margin-right: 10px;
}
.sn-table tr:nth-child(odd){
    background:#F9FBFC;
}

.sn-table-btn-group {
    width: 35%
}

.sn-table-pager {
    width: 65%
}

.sn-table-pager ul {
    margin: 0
}

.table-text {
    margin-top: 10px;
    font-size: 12px;
    color: #333
}

.table-text a {
    margin-left: 20px;
    color: #3a8cbb;
    cursor: pointer
}


.error {
    color: #f60;
}

.success {
    color: #78be25;
}


.clear {
    clear: both
}

.btn{
    padding: 6px 12px!important;
}

.text_overflow {
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    width: 100px;
}
.text_change {
    width:700px;
    word-break:break-all;
}
.label-style{
