
// override bootstrap
@brand-primary: #3a8cbb;
@brand-success: #83ae36;
@brand-danger: #d95208;

// end override

// override adminlte
@body-bg: #fff;
@light-blue: #3a8cbb;
@green: #83ae36;
@red: #f95208;


@btn-border-radius: 2px;
@btn-lg-border-radius: 3px;
@btn-sm-border-radius: 3px;
@btn-xs-border-radius: 2px;


@btn-lg-font-size: 16px;
@btn-sm-font-size: 16px;
@btn-xs-font-size: 14px;

// end override

@font-base-color: #000;
@font-title-color: lighten(@font-base-color, 20%);
@font-text-color: lighten(@font-base-color, 40%);
@font-disabled-color: lighten(@font-base-color, 60%);
@font-white-color: #fff;

@font-size-normal: 14px;
@font-size-small: 12px;

@panel-bg: #f2f2f2;
@box-border-color: #ddd;
@panel-item-hover: #d7e8f2;